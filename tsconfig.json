{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@ui": ["./components/shared"],
      "@ui/*": ["./components/shared/*"],
      "@layout": ["./layout"],
      "@layout/*": ["./layout/*"],
      "@container": ["./container"],
      "@container/*": ["./container/*"],
      "@flow": ["./components/private/flow"],
      "@flow/*": ["./components/private/flow/*"],
      "@form": ["./components/private/form"],
      "@form/*": ["./components/private/form/*"],
      "@list": ["./components/private/list"],
      "@list/*": ["./components/private/list/*"],
      "@profile": ["./components/private/profile"],
      "@profile/*": ["./components/private/profile/*"],
      "@table": ["./components/private/table"],
      "@table/*": ["./components/private/table/*"],
      "@section": ["./components/private/section"],
      "@section/*": ["./components/private/section/*"],
      "@context": ["./contexts"],
      "@context/*": ["./contexts/*"],
      "@api": ["./services"],
      "@api/*": ["./services/*"],
      "@interface": ["./interfaces"],
      "@interface/*": ["./interfaces/*"],
      "@hook": ["./hooks"],
      "@hook/*": ["./hooks/*"],
      "@helper": ["./helpers"],
      "@helper/*": ["./helpers/*"],
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
